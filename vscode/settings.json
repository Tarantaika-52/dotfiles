{
	"telemetry.telemetryLevel": "off",
	"rust-analyzer.procMacro.enable": true,
	"rust-analyzer.check.command": "clippy",
	"rust-analyzer.checkOnSave": true,
	"window.title": "${rootName}",
	"editor.minimap.enabled": false,
	"workbench.tips.enabled": false,
	"workbench.editor.editorActionsLocation": "hidden",
	"explorer.decorations.colors": false,
	"workbench.editor.empty.hint": "hidden",
	"workbench.editor.enablePreview": false,
	"workbench.settings.editor": "json",
	"editor.tabSize": 2,
	"workbench.editor.showTabs": "single",
	"workbench.editor.tabSizing": "shrink",
	"window.restoreWindows": "folders",
	"window.density.editorTabHeight": "compact",
	"editor.lineNumbers": "on",
	"window.zoomLevel": 0.65,
	"editor.suggest.preview": true,
	"workbench.activityBar.location": "hidden",
	"workbench.startupEditor": "none",
	"diffEditor.renderSideBySide": false,
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.fontSize": 18,
	"editor.fontWeight": 400,
	"editor.fontLigatures": false,
	"editor.fontFamily": "JetBrainsMono-Regular, Menlo, Monaco, 'Courier New', monospace",
	"editor.scrollBeyondLastLine": true,
	"editor.suggestSelection": "first",
	"editor.folding": false,
	"editor.insertSpaces": false,
	"editor.smoothScrolling": true,
	// "editor.wordWrap": "bounded",
	// "editor.wrappingIndent": "same",
	// "editor.wordWrapColumn": 80,
	"editor.bracketPairColorization.enabled": false,
	"editor.glyphMargin": false,
	"editor.scrollbar.horizontal": "hidden",
	"editor.scrollbar.vertical": "hidden",
	"editor.linkedEditing": true,
	"editor.renderControlCharacters": false,
	"editor.colorDecorators": false,
	"editor.stickyScroll.scrollWithEditor": false,
	"editor.scrollbar.ignoreHorizontalScrollbarInContentHeight": true,
	"editor.accessibilitySupport": "off",
	"editor.copyWithSyntaxHighlighting": false,
	"editor.stickyScroll.enabled": false,
	"editor.cursorBlinking": "expand",
	"editor.cursorStyle": "line-thin",
	"editor.cursorSmoothCaretAnimation": "explicit",
	"terminal.integrated.tabs.defaultColor": "terminal.ansiWhite",
	"terminal.integrated.fontSize": 16,
	"terminal.integrated.smoothScrolling": true,
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.showLinkHover": false,
	"explorer.compactFolders": false,
	"html.autoClosingTags": true,
	"typescript.autoClosingTags": true,
	"window.commandCenter": false,
	"workbench.layoutControl.enabled": false,
	"workbench.iconTheme": "catppuccin-perfect-monokai",
	"workbench.colorTheme": "Bearded Theme Monokai Reversed",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"workbench.panel.showLabels": false,
	"breadcrumbs.enabled": false,
	"breadcrumbs.icons": false,
	"breadcrumbs.showKeys": false,
	"breadcrumbs.showFiles": false,
	"workbench.statusBar.visible": false,
	"editor.renderWhitespace": "none",
	"extensions.ignoreRecommendations": true,
	"chat.commandCenter.enabled": false,
	"cSpell.language": "en,ru",
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"prettier.useTabs": true,
	"prettier.semi": false,
	"prettier.jsxSingleQuote": true,
	"prettier.singleQuote": true,
	"prettier.arrowParens": "avoid",
	"editor.quickSuggestions": {
		"strings": "on"
	},
	"editor.scrollbar.verticalScrollbarSize": 0,
	"workbench.sideBar.location": "right",
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer",
		"editor.tabSize": 2,
		"editor.formatOnSave": true
	},
	"explorer.fileNesting.patterns": {
		"*.ts": "${capture}.js",
		"*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
		"*.jsx": "${capture}.js",
		"*.tsx": "${capture}.ts",
		"tsconfig.json": "tsconfig.*.json",
		"package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
		"Cargo.toml": "Cargo.lock",
		"*.sqlite": "${capture}.${extname}-*",
		"*.db": "${capture}.${extname}-*",
		"*.sqlite3": "${capture}.${extname}-*",
		"*.db3": "${capture}.${extname}-*",
		"*.sdb": "${capture}.${extname}-*",
		"*.s3db": "${capture}.${extname}-*"
	},
	"explorer.confirmDelete": false
}
